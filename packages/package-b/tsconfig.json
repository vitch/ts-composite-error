{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "../../tsconfig.compiler-options.json",
  "compilerOptions": {
    "composite": true,
    "baseUrl": "./src",
    "declarationDir": "./declarations",
    "paths": {
      "ts-composite-error-b": ["."],
      "ts-composite-error-b/*": ["./*"],
    }
  },
  "include": ["./src"]
}
  